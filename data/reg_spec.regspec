# Chip register specification

REG CTRL 0x00 "Control register"
    FIELD ENABLE    0   1   "Enable module"
    FIELD MODE      1   2   "Operating mode"

REG STATUS 0x04 "Status register"
    FIELD READY     0   1   "Ready flag"
    FIELD ERROR     1   1   "Error flag"

REG reg1 0x08 "new register added"
    FIELD f1     0   1   "field f1"
    FIELD f2     1   1   "field f2"
